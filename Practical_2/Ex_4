#include <iostream>
using namespace std;

void floyd_traingle(int n){ 
    int a = 1; 
    for(int i=1; i <= n; i++){ 
        for(int c=1; c <=i; c++){ 
            cout << a << " ";
            a++;
        }
        cout << endl;
    }
}

int main(){
    int n; 
    cout << "Enter the number of lines: ";
    cin >> n; 
    cout << endl;
    
    floyd_traingle(n); 
    cout << endl;
    
    return 0;
}