#include <iostream>
#include <cmath>
using namespace std;

// declaration of function "is in the circle or not"
int distance_circle (float);

// definition of function 'distance_circle'
int distance_circle (float x, float y, float r,float x1, float x2){
  float z = sqrt((x-x1)*(x-x1)+(y-x2)*(y-x2));
}

int main (){
  float x, y, r, x1, x2;
  cout << "x, y, r, x1, x2";
  cin >> x >> y >> r >> x1 >> x2;

  if (distance_circle(x, y, r, x1, x2) <= r){
    cout << "The point is inside the circle";
  }
  else{
    cout << "The point is outside the circle";
  }
  return 0;
}
