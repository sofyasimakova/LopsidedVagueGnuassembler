#include<iostream>
using namespace std;

void swap(int x,int y)
{
    int a; 
    a = x; 
    x = y; 
    y = a; 
    cout << "The new value of x is: " << x << endl;
    cout << "The new value of y is: " << y << endl;
    
}

int main()
{
    int x,y; 
    cout << "Enter a value for x: ";
    cin >> x; 
    cout << "Enter a value for y: ";
    cin >> y;
    swap(x,y);
    return 0;
}